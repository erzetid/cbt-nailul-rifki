{
  "name": "express-mongo",
  "version": "1.0.0",
  "description": "Boilerplate Express Mongo",
  "type": "module",
  "private": true,
  "main": "app.js",
  "author": "Muhammad Andrii Fahrizal",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist/main.cjs",
    "client": "npm start --prefix client",
    "server": "nodemon app.js",
    "test": "concurrently \"jest --watch --verbose\" \"npm run test --prefix client\"",
    "dev": "export NODE_ENV=development && concurrently \"nodemon\" \"npm run client\"",
    "build:server": "webpack --mode production",
    "build:all": "rm -rf dist/ && concurrently \"webpack --mode production\" \"npm run build --prefix client\"",
    "password": "node password.js"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connected-react-router": "^6.9.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^15.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "history": "^5.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.10",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^7.32.0",
    "javascript-obfuscator": "^3.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.0"
  }
}
